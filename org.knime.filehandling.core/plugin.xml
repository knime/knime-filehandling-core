<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.knime.filehandling.core.MountPointIDProvider" name="MountPointIDProvider" schema="schema/org.knime.filehandling.core.MountPointIDProvider.exsd"/>
   <extension-point id="org.knime.filehandling.FSTestInitializerProvider" name="FSTestInitializerProvider" schema="schema/org.knime.filehandling.core.FSTestInitializerProvider.exsd"/>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/cfilehandling16x16.png"
            level-id="file-handling-prototype"
            locked="true"
            name="File Handling (Labs)"
            path="/labs/">
      </category>
   </extension>
   <extension
         point="org.knime.core.PortType">
      <portType
            color="#178BA5"
            hidden="false"
            name="File System"
            objectClass="org.knime.filehandling.core.port.FileSystemPortObject"
            objectSerializer="org.knime.filehandling.core.port.FileSystemPortObject$Serializer"
            specClass="org.knime.filehandling.core.port.FileSystemPortObjectSpec"
            specSerializer="org.knime.filehandling.core.port.FileSystemPortObjectSpec$Serializer">
      </portType>
   </extension>
   <extension
         point="org.knime.core.DataType">
      <DataType
            cellClass="org.knime.filehandling.core.data.path.cell.FSPathCell">
         <serializer
               cellClass="org.knime.filehandling.core.data.path.cell.FSPathCell"
               serializerClass="org.knime.filehandling.core.data.path.cell.FSPathCell$PathSerializer">
         </serializer>
      </DataType>
   </extension>
   <extension
         point="org.knime.core.DataColumnMetaDataType">
      <DataColumnMetaData
            dataColumnMetaDataExtension="org.knime.filehandling.core.data.path.FSPathValueMetaDataExtension">
      </DataColumnMetaData>
   </extension>
   <extension
         point="org.knime.core.DataValueRenderer">
      <dataValue
            valueClass="org.knime.filehandling.core.data.path.FSPathValue">
         <renderer
               rendererFactoryClass="org.knime.filehandling.core.data.path.FSPathValueRenderer$DefaultRendererFactory"
               suggestAsDefault="true">
         </renderer>
      </dataValue>
   </extension>

</plugin>
